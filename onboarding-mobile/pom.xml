<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <groupId>com.tandt53</groupId>
        <artifactId>automation</artifactId>
        <version>1.0.3</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>onboarding-mobile</artifactId>
    <name>onboarding-mobile</name>
    <properties>
        <maven.compiler.source>8</maven.compiler.source>
        <maven.compiler.target>8</maven.compiler.target>
    </properties>

    <dependencies>

        <dependency>
            <groupId>com.tandt53</groupId>
            <artifactId>onboarding-common</artifactId>
            <version>1.0.3</version>
        </dependency>

        <dependency>
            <groupId>com.tandt53</groupId>
            <artifactId>onboarding-common-test</artifactId>
            <version>1.0.3</version>
        </dependency>

        <dependency>
            <groupId>com.tandt53</groupId>
            <artifactId>onboarding-ui</artifactId>
            <version>1.0.3</version>
        </dependency>

        <dependency>
            <groupId>com.tandt53</groupId>
            <artifactId>onboarding-data-provider</artifactId>
            <version>1.0.3</version>
        </dependency>

        <dependency>
            <groupId>io.appium</groupId>
            <artifactId>java-client</artifactId>
        </dependency>

        <dependency>
            <groupId>com.google.guava</groupId>
            <artifactId>guava</artifactId>
        </dependency>

        <dependency>
            <groupId>com.google.inject</groupId>
            <artifactId>guice</artifactId>
        </dependency>

        <dependency>
            <groupId>org.seleniumhq.selenium</groupId>
            <artifactId>selenium-support</artifactId>
        </dependency>

    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>